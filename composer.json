{
    "name": "aticmatic/envatophp",
    "description": "A Laravel package for interacting with the Envato API.",
    "type": "library",
    "license": "MIT", // Choose your license (e.g., MIT, GPL, etc.)
    "authors": [
        {
            "name": "AticmAtic",
            "email": "contact@aticmatic.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "AticMatic\\EnvatoPHP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AticMatic\\EnvatoPHP\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "AticMatic\\EnvatoPHP\\EnvatoPHPServiceProvider"
            ],
            "aliases": {
                "EnvatoPHP": "AticMatic\\EnvatoPHP\\Facades\\EnvatoPHP"
            }
        }
    }
}